<?xml version="1.0" encoding="UTF-8"?>

<hibernate-mapping package="usos.database">

	
	<class table="ELEMENT" name="Element">
		<id name="id" column="ELEMENT_ID">
			<generator class="native"/>
		</id>
		
		<property name="date" column="ELEMENT_DATE" type="timestamp"/>
		<property name="data" type="text"/>
		<property name="login"/>
	</class>

</hibernate-mapping>